from tkinter import *
from random import * 

class Start():
  def __init__(self):
    self.root =Tk()
    self.can = Canvas(self.root, height = 500, width= 500)
    self.b = Button(self.root, text='start', command=self.StartGame)
    self.b.pack()
    
 def MakeBoard():
    self.list = []
    for r in range(11):
      for n in range(11):
        self.list[n].append(0)
    for i in range(2):
      self.list[random.randint(144)] = 1
     
 def Player():
  move = input('Vyber cislo 0-143')
  while move<-1 or move>144:
    move = input()
    
  
  
 def rb(self, ):
    for r in MakeBoard.self.list:
      if r==0:
        print('*')
        
        
    
    
 def StartGame():
    self.Board = MakeBoard()
    self.RealBoard = rb()
    self.hrac = Player()
    
    
