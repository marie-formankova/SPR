 def Player():
  self.move = input('Vyber cislo 0-143')
  while self.move<-1 or self.move>144:
    self.move = input()
  self.moving = Moving(self.move, MakeBoard.self.list)
    
   
  
  
 def rb():
    for r in MakeBoard.self.list:
      if r==0:
        print('.')
      elif r==2:
        print('*')
      
        
        
    
    
 def StartGame():
    self.Board = MakeBoard()
    self.RealBoard = rb()
